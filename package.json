{
  "name": "omega-ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
